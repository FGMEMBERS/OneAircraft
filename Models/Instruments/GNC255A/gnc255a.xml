<!--Comm/Nav Button-->

  <animation>
    <type>pick</type>
    <object-name>c/n</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/gnc255a/comm-mode</property>
      </binding>
</action>
