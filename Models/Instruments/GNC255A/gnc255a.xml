  <!-- actions -->

  <!-- Comm part -->

  <animation>
    <type>knob</type>
    <object-name>comvol</object-name>
    <property alias="../../params/comm-volume"/>
    <factor>-65</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.01473</x-m>
      <y-m>-0.06498</y-m>
      <z-m>0.00962</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../params/comm-volume"/>
        <factor>-0.1</factor>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
